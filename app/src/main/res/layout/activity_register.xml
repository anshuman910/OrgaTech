<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/guestwall"
    android:orientation="vertical"
    tools:context=".Register">



    <LinearLayout
        android:id="@+id/lin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="6dp">


        <ImageView
            android:id="@+id/reglogo"
            android:layout_width="166dp"
            android:layout_height="134dp"
            app:srcCompat="@drawable/logo_final"
            tools:layout_editor_absoluteX="123dp"
            tools:layout_editor_absoluteY="4dp"
            android:layout_gravity="center"
            android:layout_marginTop="8dp"

            android:layout_marginBottom="8dp"/>

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gudashlogo">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:background="@drawable/rounded_corner_white"
                    android:foregroundGravity="center"
                    android:layout_gravity="center"
                    android:text="Registration page"
                    android:textColor="#000000"
                    android:textSize="30dp"
                    tools:layout_editor_absoluteX="87dp"
                    tools:layout_editor_absoluteY="154dp" />

                <EditText
                    android:id="@+id/editText"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Full Name"
                    tools:layout_editor_absoluteY="208dp" />

                <EditText
                    android:id="@+id/editText5"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Email"
                    tools:layout_editor_absoluteY="239dp" />

                <EditText
                    android:id="@+id/editText3"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Phone Number"
                    tools:layout_editor_absoluteY="272dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/rounded_corner_white"
                        android:foregroundGravity="center"
                        android:text="D.O.B."
                        android:textColor="#000000"
                        android:textSize="25dp"
                        tools:layout_editor_absoluteX="33dp"
                        tools:layout_editor_absoluteY="303dp" />

                    <Button
                        android:id="@+id/button2"
                        android:layout_width="88dp"
                        android:layout_height="34dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/cal"
                        tools:layout_editor_absoluteX="122dp"
                        tools:layout_editor_absoluteY="306dp" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/rounded_corner_white"
                        android:text="Gender"
                        android:textColor="#000000"
                        android:textSize="24sp"
                        tools:layout_editor_absoluteX="33dp"
                        tools:layout_editor_absoluteY="345dp" />

                    <Spinner
                        android:id="@+id/spinner2"
                        android:layout_width="83dp"
                        android:layout_height="32dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="3dp"
                        android:background="@drawable/rounded_corner_white"
                        android:entries="@array/gen"
                        tools:layout_editor_absoluteX="127dp"
                        tools:layout_editor_absoluteY="346dp" />


                </LinearLayout>

                <EditText
                    android:id="@+id/editText2"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Address"
                    tools:layout_editor_absoluteY="385dp" />

                <EditText
                    android:id="@+id/editText6"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Password"
                    tools:layout_editor_absoluteY="412dp" />

                <EditText
                    android:id="@+id/loginpass"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Re-enter Password"
                    tools:layout_editor_absoluteY="444dp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/course"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/rounded_corner_white"
                        android:text="COURSE"
                        android:textColor="#000000"
                        android:textSize="25dp"
                        tools:layout_editor_absoluteX="24dp"
                        tools:layout_editor_absoluteY="475dp" />

                    <Spinner
                        android:id="@+id/cour"
                        android:layout_width="82dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="3dp"
                        android:background="@drawable/rounded_corner_white"
                        android:entries="@array/course"
                        tools:layout_editor_absoluteX="138dp"
                        tools:layout_editor_absoluteY="475dp" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/stream"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/rounded_corner_white"
                        android:text="STREAM"
                        android:textColor="#000000"
                        android:textSize="25dp"
                        tools:layout_editor_absoluteX="28dp"
                        tools:layout_editor_absoluteY="527dp" />

                    <Spinner
                        android:id="@+id/str"
                        android:layout_width="82dp"
                        android:layout_height="35dp"
                        android:layout_marginLeft="17dp"
                        android:layout_marginTop="3dp"
                        android:background="@drawable/rounded_corner_white"
                        android:entries="@array/stream"
                        tools:layout_editor_absoluteX="139dp"
                        tools:layout_editor_absoluteY="531dp" />


                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:text="Please select a security question and give the corresponding answer (case sensitive) "
                    android:textColor="#000000" />

                <Spinner
                    android:id="@+id/regseqsp"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"

                    />

                <EditText
                    android:id="@+id/regseqed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rounded_corner_white"
                    android:hint="Enter your answer (Case sensitive)" />


                <Button
                    android:id="@+id/regsubmit"
                    android:layout_width="200dp"
                    android:layout_height="66dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/round_button_3"
                    android:text="SUBMIT"
                    android:textSize="20dp"
                    android:layout_marginBottom="8dp"
                    tools:layout_editor_absoluteX="105dp"
                    tools:layout_editor_absoluteY="649dp" />

            </LinearLayout>


        </ScrollView>


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>